{
  "name": "egg-mysql-sequelize-auto",
  "version": "0.1.1",
  "description": "Automatically generate mysql sequelize models for Egg.js framework from your database.",
  "main": "index.js",
  "devDependencies": {
    "fs-extra": "^9.0.1",
    "@types/fs-extra": "^9.0.4",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.14.7",
    "@types/lodash": "^4.14.108",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "typescript": "^4.0.2",
    "rimraf": "^3.0.2",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "yargs": "^16.2.0"
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf lib && rimraf types && rimraf **/model && rimraf *.tgz",
    "build": "npm run clean && tsc",
    "dev": "tsc && mysql-sequelize-model -o './model' -h 127.0.0.1 -d admin_demo_local -u test -p 3306 -x 123 -t system_user -f"
  },
  "engines": {
    "node": ">= 8"
  },
  "bin": {
    "sequelize-auto": "bin/sequelize-auto"
  },
  "files": [
    "*.js",
    "lib/**/*.js",
    "types/**/*.ts",
    "stubs/*.stub",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerryhu/egg-mysql-sequelize-auto.git"
  },
  "bugs": {
    "url": "https://github.com/jerryhu/egg-alicloud-mns/issues"
  },
  "homepage": "https://github.com/jerryhu/egg-mysql-sequelize-auto#readme",
  "author": "Jerry Hu",
  "license": "MIT"
}
